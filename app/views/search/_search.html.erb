<div class="search-form">
	<%= form_tag(result_path,:method => 'get') do %>
	  <%= select_tag :model, options_for_select({"User" => "User", "Blog" => "Post", "Point" => "Point"}) %>
	  <%= text_field_tag :search,'' %>
	  <%= submit_tag '検索', :name => nil, class: "btn btn-xs btn-default" %>
	<% end %>
</div>